<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Rimworld: Spartan Foundry</li>
		</mods>
		<match Class="PatchOperationSequence">
		<operations>		
	<!-- =============== Spacer Tag Categories, Recon, Samurai, Scout, Shock ===============-->	
			<li Class="PatchOperationAdd">		
				<xpath>/Defs/ThingDef[defName = "SF_ReconPowerArmor" or defName= "SF_SamuraiPowerArmor" or defName= "SF_ScoutPowerArmor" or defName= "SF_ShockTrooperArmor"]</xpath>
				<value>
					<techLevel>Spacer</techLevel>
				</value>
			</li>	  
			
		</operations>		
		</match>
	</Operation>
	
</Patch> 

